### Option:Plugins.PostgreSQL.System.Path
#	Path to external plugin executable.
#
# Mandatory: yes
# Default:
Plugins.PostgreSQL.System.Path=/usr/sbin/zabbix-agent2-plugin/zabbix-agent2-plugin-postgresql
{% if param_plugins_postgresql_system_path  is defined and param_plugins_postgresql_system_path %}

Plugins.PostgreSQL.System.Path={{ param_plugins_postgresql_system_path }}
{% endif %}

