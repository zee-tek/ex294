### Option: Plugins.PostgreSQL.Sessions.*.Uri
#	Uri to connect. "*" should be replaced with a session name.
#
# Mandatory: no
# Range:
#   Must match the URI format.
#   Supported schemas: "tcp" and "unix".
#   Embedded credentials will be ignored.
# Default:
# Plugins.PostgreSQL.Sessions.*.Uri=
{% if param_plugins_postgresql_sessions is defined and param_plugins_postgresql_sessions is iterable and param_plugins_postgresql_sessions is not string and param_plugins_postgresql_sessions is not mapping %}

{% for session in param_plugins_postgresql_sessions %}
{% if session["uri"] is defined and session["uri"] %}
Plugins.PostgreSQL.Sessions.{{ session["name"] }}.Uri={{ session["uri"] }}
{% endif %}
{% endfor %}
{% endif %}

